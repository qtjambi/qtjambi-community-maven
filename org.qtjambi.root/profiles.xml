<profilesXml xmlns="http://maven.apache.org/PROFILES/1.0.0" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
	xsi:schemaLocation="http://maven.apache.org/PROFILES/1.0.0 http://maven.apache.org/xsd/profiles-1.0.0.xsd">

	<profiles>
		<profile>
			<id>release</id>
			<properties>
				<qmake.make.target></qmake.make.target>
			</properties>
		</profile>
		<profile>
			<id>debug</id>
			<properties>
				<qmake.make.target>debug</qmake.make.target>
			</properties>
		</profile>
		<profile>
			<id>linux-i386-gcc</id>
			<properties>
				<build.os>linux</build.os>
				<build.arch>i386</build.arch>
				<build.linker>gcc</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
		<profile>
			<id>linux-amd64-gcc</id>
			<properties>
				<build.os>linux</build.os>
				<build.arch>amd64</build.arch>
				<build.linker>gcc</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
		<profile>
			<id>windows-x86-mingw</id>
			<properties>
				<build.os>windows</build.os>
				<build.arch>x86</build.arch>
				<build.linker>mingw</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
		<profile>
			<id>windows-x86-msvc2010</id>
			<properties>
				<build.os>windows</build.os>
				<build.arch>x86</build.arch>
				<build.linker>msvc2010</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
		<profile>
			<id>windows-amd64-mingw_w64</id>
			<properties>
				<build.os>windows</build.os>
				<build.arch>amd64</build.arch>
				<build.linker>mingw_w64</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
		<profile>
			<id>windows-amd64-msvc2010</id>
			<properties>
				<build.os>windows</build.os>
				<build.arch>amd64</build.arch>
				<build.linker>msvc2010</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
		<profile>
			<id>macosx-intel-xcode</id>
			<properties>
				<build.os>macosx</build.os>
				<build.arch>intel</build.arch>
				<build.linker>xcode</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
		<profile>
			<id>macosx-universal-xcode</id>
			<properties>
				<build.os>macosx</build.os>
				<build.arch>universal</build.arch>
				<build.linker>xcode</build.linker>
				<build.oal>${build.os}.${build.arch}.${build.linker}</build.oal>
			</properties>
		</profile>
	</profiles>

</profilesXml>
